<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AudioTextureView">
        <attr name="total" format="integer"/>
        <attr name="min" format="integer"/>
        <attr name="max" format="integer"/>
        <attr name="mode">
            <flag name="round" value="0"/>
            <flag name="rect" value="1"/>
            <flag name="line" value="2"/>
        </attr>
        <attr name="drawOffset" format="dimension"/>
        <attr name="drawHeight" format="dimension"/>
        <attr name="auto" format="boolean"/>
        <attr name="colors" format="string"/>
    </declare-styleable>
</resources>